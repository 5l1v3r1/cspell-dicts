{
  "name": "cspell-dict-lua",
  "version": "1.0.0",
  "description": "Lua dictionary for cspell.",
  "bin": {
    "cspell-dict-lua-link": "./link.js",
    "cspell-dict-lua-unlink": "./unlink.js"
  },
  "scripts": {
    "build": "cspell-tools compile \"lua.txt\" -o .",
    "prepublishOnly": "cp ../../util/* .",
    "cspell-link": "node link.js",
    "cspell-unlink": "node unlink.js",
    "publish-minor": "npm version minor && npm publish",
    "publish-patch": "npm version patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jason3S/cspell-dicts.git"
  },
  "keywords": [
    "cspell",
    "lua",
    "lua",
    "dictionary",
    "spelling"
  ],
  "main": "index.js",
  "typings": "index.d.ts",
  "author": "Jason Dent & Simone Livieri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jason3S/cspell-dicts/issues"
  },
  "homepage": "https://github.com/Jason3S/cspell-dicts/blob/master/lua/README.md",
  "devDependencies": {
    "cspell-tools": "^3.0.5"
  },
  "dependencies": {
    "configstore": "^4.0.0"
  },
  "files": [
    "lua.txt.gz",
    "cspell-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
