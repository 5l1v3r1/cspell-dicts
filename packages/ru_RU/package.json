{
  "name": "cspell-dict-ru_ru",
  "version": "1.1.5",
  "description": "Russian dictionary for cspell.",
  "bin": {
    "cspell-dict-ru-ru-link": "./link.js",
    "cspell-dict-ru-ru-unlink": "./unlink.js"
  },
  "scripts": {
    "build": "cspell-tools compile-trie \"ru_ru.dic\" -o .",
    "prepublishOnly": "cp ../../util/* .",
    "cspell-link": "node link.js",
    "cspell-unlink": "node unlink.js",
    "publish-minor": "npm version minor && npm publish",
    "publish-patch": "npm version patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jason3S/cspell-dicts.git"
  },
  "keywords": [
    "cspell",
    "ru_RU",
    "Russian",
    "dictionary",
    "spelling"
  ],
  "main": "index.js",
  "typings": "index.d.ts",
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jason3S/cspell-dicts/issues"
  },
  "homepage": "https://github.com/Jason3S/cspell-dicts/blob/master/ru_RU/README.md",
  "devDependencies": {
    "cspell-tools": "^3.0.3"
  },
  "dependencies": {
    "configstore": "^3.1.2"
  },
  "files": [
    "LICENSE.ru_ru.txt",
    "ru_ru.trie.gz",
    "cspell-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
