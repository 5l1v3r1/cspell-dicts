{
  "name": "cspell-dict-it-it",
  "version": "1.0.0",
  "description": "Italian dictionary for cspell. -- Private until verified",
  "private": true,
  "bin": {
    "cspell-dict-it-it-link": "./link.js",
    "cspell-dict-it-it-unlink": "./unlink.js"
  },
  "scripts": {
    "build": "cspell-tools compile-trie \"it_IT.dic\" -o .",
    "test": "head -n 100 \"it_IT.dic\" | cspell -v -c ./cspell-ext.json \"--local=it,it-IT\" \"--languageId=*\" stdin",
    "cspell-link": "node link.js",
    "cspell-unlink": "node unlink.js",
    "prepare": "cp ../../util/* . && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streetsidesoftware/cspell-dicts.git"
  },
  "keywords": [
    "cspell",
    "it_IT",
    "Italian",
    "dictionary",
    "spelling"
  ],
  "main": "index.js",
  "typings": "index.d.ts",
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell-dicts/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell-dicts/blob/master/packages/it_IT#readme",
  "devDependencies": {},
  "dependencies": {
    "configstore": "^4.0.0"
  },
  "files": [
    "it_IT.trie.gz",
    "cspell-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
